    "@types/node": "^18.11.10",
    "@types/prismjs": "^1.26.0",
    "@types/pug": "^2.0.6",
    "@typescript-eslint/parser": "^5.45.1",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.11.2",
    "colors": "^1.4.0",
    "fs-extra": "^11.1.0",
    "jsdoc": "^4.0.0",
    "jsdoc-parse": "^6.2.0",
    "lodash": "^4.17.21",
    "parse-options": "^1.0.6",
    "prismjs": "^1.29.0",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "showdown": "^2.1.0",
