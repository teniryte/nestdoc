    "webpack": "^5.75.0"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.test.ts'",
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rm -rf ./dist",
    "copy-files": "cp -r ./assets/ ./dist/assets",
    "dev": "nodemon --exec \"bash \" ./scripts/build --watch './src/*' --watch './assets/*' --watch './lib' -e js,pug,ts,css,styl,png"
  },
  "author": {
    "name": "teniryte",
    "email": "teniryte@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:teniryte/nestdoc.git"
  },
  "publish": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "nestdoc": "./scripts/run"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.1.0"
